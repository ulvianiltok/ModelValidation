@model Kullanici
<form action="/Kullanici/Ekle" method="GET">
    <div class="form-group">
        <label>Ad</label>
        <input name="Ad" class="form-control">
        <span asp-validation-for="Ad" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Soyad</label>
        <input name="Soyad"class="form-control"/>
        <span asp-validation-for="Ad" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>E-posta</label>
        <input name="Eposta" class="form-control"/>
        <span asp-validation-for="Eposta" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Telefon</label>
        <input name="Telefon" class="form-control">
        <span asp-validation-for="Telefon" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Sifre</label>
        <input name="Sifre" class="form-control">
        <span asp-validation-for="Sifre" class="text-danger"></span>
    </div>
        <div class="form-group">
        <label>SifreTekrar</label>
        <input name="SifreTekrar" class="form-control">
        <span asp-validation-for="SifreTekrar" class="text-danger"></span>
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-primary">Gönder</button>
    </div>
</form>